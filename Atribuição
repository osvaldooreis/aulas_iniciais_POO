public class Atribuição {
    public static void main(String[] args) {


        String nome_da_pessoa;
        int idade_da_pessoa;
        char sexo_da_pessoa;

        nome_da_pessoa = "Osvaldo Filho";
        idade_da_pessoa = 39;
        sexo_da_pessoa = 'M';


        System.out.println("Nome: " + nome_da_pessoa);
        System.out.println("Idade: " + idade_da_pessoa + " anos");
        System.out.println("Sexo: " + sexo_da_pessoa);;


        // System.out.println("Idade:" + idade);


        }
    }
